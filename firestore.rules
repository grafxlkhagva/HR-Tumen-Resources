/**
 * WARNING: These rules are intentionally insecure for development purposes.
 * Any authenticated user can read and write to the entire database.
 * DO NOT use these rules in a production environment.
 */
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read and write access to any document for any signed-in user.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
